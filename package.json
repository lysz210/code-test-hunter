  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node .webpack/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:server && npm run pack:client",
    "pack:server": "cross-env NODE_ENV=production webpack --progress --colors --config .webpack/webpack.server.config.js",
    "pack:client": "cross-env NODE_ENV=production webpack --progress --colors --config .webpack/webpack.client.config.js",
    "test": "npm run unit",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix"
  },
